properties:
  file.name:
    displayName: Province
views:
  - type: table
    name: Table
    filters:
      and:
        - file.tags.containsAny("Province", "Territory")
        - file.inFolder("World/Continents/Eistrin/States/Kingdom of Teryn/Provinces/Northern Territory")
    order:
      - file.name
      - population
      - economy
    sort:
      - property: tags
        direction: DESC
    columnSize:
      file.name: 168
      note.population: 123
